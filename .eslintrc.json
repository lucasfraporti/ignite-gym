{
	"env": {
			"es2021": true,
			"node": true
	},
	"extends": [
			"@rocketseat/eslint-config/react",
			"plugin:react/recommended"
	],
	"overrides": [
			{
					"extends": [
							"@rocketseat/eslint-config/react"
					],
					"files": [
							"*.ts",
							"*.tsx"
					]
			}
	],
	"parserOptions": {
			"ecmaVersion": "latest",
			"sourceType": "module"
	},
	"plugins": [
			"react"
	],
	"rules": {
			"camelcase": "warn"
	}
}
